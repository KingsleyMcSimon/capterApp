<p id="notice"><%= notice %></p>

<h1>Capts</h1>

<table>
  <thead>
    <tr>
      <th>Capt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @capts.each do |capt| %>
      <tr>
        <td><%= capt.capt %></td>
        <td><%= link_to 'Show', capt %></td>
        <td><%= link_to 'Edit', edit_capt_path(capt) %></td>
        <td><%= link_to 'Destroy', capt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Capt', new_capt_path %>
